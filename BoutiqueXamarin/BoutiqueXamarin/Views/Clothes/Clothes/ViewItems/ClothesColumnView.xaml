<?xml version="1.0" encoding="UTF-8"?>
<views:ClothesColumnBase xmlns="http://xamarin.com/schemas/2014/forms" 
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="BoutiqueXamarin.Views.Clothes.Clothes.ViewItems.ClothesColumnView"
             xmlns:views="clr-namespace:BoutiqueXamarin.Views.Clothes.Clothes.ViewItems;assembly=BoutiqueXamarin"
             xmlns:viewModelItems="clr-namespace:BoutiqueXamarin.ViewModels.Clothes.Clothes.ClothesViewModelItems;assembly=BoutiqueXamarin"
             x:DataType="{x:Type viewModelItems:ClothesColumnViewModelItem}">
    <Grid Margin="0, 0, 0, 20">
        <Grid.ColumnDefinitions>
            <ColumnDefinition />
            <ColumnDefinition />
        </Grid.ColumnDefinitions>

        <views:ClothesItemView x:Name="ItemLeft" 
                               Grid.Column="0"/>
        <views:ClothesItemView x:Name="ItemRight" 
                               Grid.Column="1"/>
    </Grid>
</views:ClothesColumnBase>
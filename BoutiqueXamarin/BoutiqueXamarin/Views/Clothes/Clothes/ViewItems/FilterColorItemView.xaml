<?xml version="1.0" encoding="UTF-8"?>
<views:FilterColorItemBase xmlns="http://xamarin.com/schemas/2014/forms" 
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="BoutiqueXamarin.Views.Clothes.Clothes.ViewItems.FilterColorItemView"
             xmlns:views="clr-namespace:BoutiqueXamarin.Views.Clothes.Clothes.ViewItems;assembly=BoutiqueXamarin"
             xmlns:viewModelItems="clr-namespace:BoutiqueXamarin.ViewModels.Clothes.Clothes.ClothesViewModelItems;assembly=BoutiqueXamarin"
             x:DataType="{x:Type viewModelItems:FilterColorViewModelItem}">
    <StackLayout Style="{StaticResource StackLayoutText}">
        <CheckBox x:Name="ColorCheck" />
        <Label x:Name="ColorName" 
               Style="{StaticResource LabelDescription}"/>
    </StackLayout>
</views:FilterColorItemBase>
<?xml version="1.0" encoding="UTF-8"?>
<views:FilterSizeItemBase xmlns="http://xamarin.com/schemas/2014/forms" 
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="BoutiqueXamarin.Views.Clothes.Clothes.ViewItems.FilterSizeItemView"
             xmlns:views="clr-namespace:BoutiqueXamarin.Views.Clothes.Clothes.ViewItems;assembly=BoutiqueXamarin"
             xmlns:viewModelItems="clr-namespace:BoutiqueXamarin.ViewModels.Clothes.Clothes.ClothesViewModelItems;assembly=BoutiqueXamarin"
             x:DataType="{x:Type viewModelItems:FilterSizeViewModelItem}">
    <StackLayout Style="{StaticResource StackLayoutText}">
        <CheckBox x:Name="SizeCheck" />
        <Label x:Name="SizeName" 
               Style="{StaticResource LabelDescription}"/>
    </StackLayout>
</views:FilterSizeItemBase>
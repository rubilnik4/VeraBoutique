<?xml version="1.0" encoding="utf-8" ?>
<TabbedPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="BoutiqueXamarin.Views.Clothes.ChoicePage"
             xmlns:viewModel="clr-namespace:BoutiqueXamarin.ViewModels.Clothes.Choice"   
             ItemsSource="{Binding ChoiceViewModelItems}" x:DataType="viewModel:ChoiceViewModel">
    <TabbedPage.ItemTemplate>
        <DataTemplate x:DataType="viewModel:ChoiceViewModelItem">
            <ContentPage Title="{Binding Name}">
            </ContentPage>
        </DataTemplate>
    </TabbedPage.ItemTemplate>
</TabbedPage>